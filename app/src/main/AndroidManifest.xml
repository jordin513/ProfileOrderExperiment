<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nidroj.profileorderexperiment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProfileOrderExperiment">
        <activity
            android:name="com.nidroj.profileorderexperiment.ui.view.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProfileOrderExperiment.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--            android:taskAffinity="com.nidroj.profileorderexperiment.ui.view.MainActivity"-->

        <activity
            android:name="com.nidroj.profileorderexperiment.ui.view.MatchDataActivity"
            android:exported="true"
            android:launchMode="singleTask"
            android:theme="@style/Theme.ProfileOrderExperiment.NoActionBar">
        </activity>
    </application>

</manifest>